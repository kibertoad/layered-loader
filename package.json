{
  "name": "layered-loader",
  "version": "14.2.0",
  "description": "Data loader with support for caching and fallback data sources ",
  "license": "MIT",
  "maintainers": [
    {
      "name": "Igor Savin",
      "email": "kibertoad@gmail.com"
    }
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:release": "del-cli dist && del-cli coverage && npm run lint && npm run build",
    "docker:start": "docker compose -f docker-compose.yml up --build -d redis valkey && docker compose -f docker-compose.yml up --build -d wait_for_redis",
    "docker:stop": "docker compose -f docker-compose.yml down",
    "test": "vitest",
    "test:everything": "npm run lint && npm run test:coverage",
    "test:coverage": "vitest --coverage",
    "lint": "biome check && tsc --noEmit",
    "lint:fix": "biome check --write",
    "prepublishOnly": "npm run build:release",
    "update:dependencies": "npx npm-check-updates -u"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/layered-loader.git"
  },
  "keywords": [
    "data",
    "loader",
    "load",
    "loading",
    "cache",
    "caching",
    "fallback",
    "alternate",
    "source",
    "redis",
    "memory",
    "fifo",
    "lru",
    "read-through",
    "refresh-ahead"
  ],
  "homepage": "https://github.com/kibertoad/layered-loader",
  "dependencies": {
    "ioredis": "^5.6.1",
    "toad-cache": "^3.7.0"
  },
  "peerDependencies": {
    "@valkey/valkey-glide": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "@valkey/valkey-glide": {
      "optional": true
    }
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.15.29",
    "@valkey/valkey-glide": "^2.1.1",
    "@vitest/coverage-v8": "^3.2.0",
    "del-cli": "^6.0.0",
    "rfdc": "^1.4.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.0"
  },
  "files": ["README.md", "LICENSE", "dist/*"]
}
