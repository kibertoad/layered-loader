{
  "name": "layered-loader",
  "version": "1.0.0",
  "description": "Data loader with support for caching and fallback data sources ",
  "license": "MIT",
  "maintainers": [
    {
      "name": "Igor Savin",
      "email": "kibertoad@gmail.com"
    }
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --config=jest.config.json",
    "test:everything": "npm run lint && npm run test:coverage",
    "test:coverage": "jest --config=jest.config.json --coverage",
    "lint": "eslint --format codeframe \"lib/**/*.ts\" \"test/**/*.ts\"",
    "prettier": "prettier --write \"{lib,test}/**/*.{js,ts}\" index.ts",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/layered-loader.git"
  },
  "keywords": [
    "data",
    "loader",
    "load",
    "cache",
    "caching",
    "fallback",
    "alternate",
    "source"
  ],
  "homepage": "http://github.com/kibertoad/layered-loader",
  "dependencies": {
    "tiny-lru": "^7.0.6"
  },
  "devDependencies": {
    "@types/jest": "26.0.23",
    "@types/node": "^15.0.2",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist/*"
  ]
}
